{
    "domains": ["en.wikipedia.org"],
    "landing_page": "landing.html",
    "https_only": true,
    "rewrite_location": true,
    "content_edits": [
        {
            "matches": ["https://en.wikipedia.org/wiki/Emacs", "https://en.wikipedia.org/wiki/Vim"],
            "override_url": "https://en.wikipedia.org/wiki/Shaitan"
        },
        {
            "matches": ["https://en.wikipedia.org/wiki/Amboise"],
            "js": "injected.js",
            "css": "style.css",
            "override_url": "https://en.wikipedia.org/wiki/Main_Page",
            "substitute": [
                {
                    "pattern": "Wikipedia",
                    "replacement": "Wikipedia (beta)",
                    "max_replacements": 1
                }
            ],
            "add_headers": {
                "Content-Security-Policy": "default-src 'self';"
            }
        },
        {
            "matches": ["https://en.wikipedia.org/wiki/*"],
            "js": "injected.js",
            "css": "pink-theme.css"
        }
    ]
}
